df = pd.read_csv('./data/LR04.csv',skiprows=4)
df.head()





lr04 = pyleo.Series(time=df.iloc[:,0],value=df.iloc[:,1],value_name='Benthic $\delta^{18}O$',value_unit=u'\u2030',
